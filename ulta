-- Similar modifications and corrections needed for the other Toggles, Slider, etc.

local Toggle3 = Tab2:CreateToggle({
   Name = "key esp",
   CurrentValue = false,
   Flag = "Toggle3",
   Callback = function(Value)
       keyesp = value
       if keyesp then
           repeat task.wait(.25)
               for i,v in pairs(bullshittable.KeyObtain) do
                   pcall(function()
                       if not v.Parent:FindFirstChild("Outline") then
                           if keyesp then
                               outline(v.Parent)
                           end
                       end
                   end)
               end
           until not keyesp
       else
           for i,v in pairs(bullshittable.KeyObtain) do
               pcall(function()
                   v.Parent.Outline:Destroy()
               end)
           end
       end
   end
}) 

local Toggle4 = Tab2:CreateToggle({
   Name = "lever esp",
   CurrentValue = false,
   Flag = "Toggle4",
   Callback = function(Value)
       leveresp = value
       if leveresp then
           repeat task.wait(.25)
               for i,v in pairs(bullshittable.LeverForGate) do
                   pcall(function()
                       if not v.Parent:FindFirstChild("Outline") then
                           if leveresp then
                               outline(v.Parent)
                           end
                       end
                   end)
               end
           until not leveresp
       else
           for i,v in pairs(bullshittable.LeverForGate) do
               pcall(function()
                   v.Parent.Outline:Destroy()
               end)
           end
       end
   end
})

