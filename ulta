-- Creating the slider for walk speed
local Slider2 = Tab2:CreateSlider({
   Name = "WALKSPEED(patched)",
   Range = {0, 100},
   Increment = 10,
   Suffix = "NOT WORKING",
   CurrentValue = 10,
   Flag = "Slider2",
   Callback = function(Value)
       getgenv().ws = value
   end
})

task.spawn(function()
   while task.wait() do
       if getgenv().wstog then
           game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = getgenv().ws
       end
   end
end)

